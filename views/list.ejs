<%- include('header'); -%>
    <div class="box">
      <h1 id="heading"><%= listHeading %> </h1>
    </div>
    <div class="box">
      <% for(var i = 0; i < tasks.length ; i++) { %>
        <div class="item">
          <input type="checkbox" name="" value="">
          <p> <%= tasks[i]  %> </p>
        </div>

      <%  }  %>

    <form class="item" action="/" method="post">
      <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
      <button type="submit" name="submit" value="<%= listHeading %>">+</button>
    </form>
    </div>
<%- include('footer'); -%>
